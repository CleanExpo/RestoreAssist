{
  "buildCommand": "cd packages/backend && npm install && npm run build",
  "installCommand": "cd packages/backend && npm install",
  "functions": {
    "packages/backend/api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/test",
      "destination": "/packages/backend/api/test"
    },
    {
      "source": "/(.*)",
      "destination": "/packages/backend/api/index"
    }
  ]
}
