{
  "version": 2,
  "buildCommand": "cd ../.. && npm install && npm run build --workspace=packages/backend",
  "installCommand": "cd ../.. && npm install",
  "framework": null,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
