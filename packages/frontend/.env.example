# Frontend Environment Variables
# Copy this file to .env.local and fill in your values
# NEVER commit .env.local to version control!

# API Configuration
VITE_API_URL=http://localhost:3001
VITE_APP_URL=http://localhost:5173

# Google OAuth (Public - safe for frontend)
# Get from: https://console.cloud.google.com/apis/credentials
# This is the CLIENT ID only - safe to expose
VITE_GOOGLE_CLIENT_ID=EXAMPLE_CLIENT_ID.apps.googleusercontent.com

# Stripe Publishable Key (Public - safe for frontend)
# Get from: https://dashboard.stripe.com/apikeys
# IMPORTANT: Only use publishable keys (pk_) in frontend!
# Test mode: pk_test_...
# Live mode: pk_live_...
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_EXAMPLE_KEY_SAFE_FOR_FRONTEND

# Stripe Product IDs (Public - safe for frontend)
# Get these from your Stripe Dashboard after creating products
VITE_STRIPE_PRODUCT_FREE_TRIAL=prod_EXAMPLE_FREE_TRIAL
VITE_STRIPE_PRODUCT_MONTHLY=prod_EXAMPLE_MONTHLY
VITE_STRIPE_PRODUCT_YEARLY=prod_EXAMPLE_YEARLY

# Stripe Price IDs (Public - safe for frontend)
# Get these from your Stripe Dashboard after creating prices
VITE_STRIPE_PRICE_FREE_TRIAL=price_EXAMPLE_FREE_TRIAL
VITE_STRIPE_PRICE_MONTHLY=price_EXAMPLE_MONTHLY
VITE_STRIPE_PRICE_YEARLY=price_EXAMPLE_YEARLY

# Sentry Error Tracking (Public DSN - safe for frontend)
# Get your DSN from https://sentry.io/settings/projects/
# Optional - leave commented to disable error tracking
# VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/your-project-id

# App Version (for Sentry release tracking)
VITE_APP_VERSION=1.0.0