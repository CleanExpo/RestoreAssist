{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:all": "npm run drive:start && npm run dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist build coverage",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:visual": "percy exec -- playwright test tests/visual",
    "test:lighthouse": "lhci autorun",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "vercel-build": "prisma generate && next build",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v",
    "docker:dev": "docker-compose up",
    "docker:db": "docker-compose up -d postgres",
    "docker:prisma-migrate": "docker-compose exec app npx prisma migrate deploy",
    "docker:prisma-studio": "docker-compose exec app npx prisma studio",
    "docker:validate": "node validate-docker.js",
    "drive:start": "cd docker && docker-compose up -d drive-resolver",
    "drive:stop": "cd docker && docker-compose stop drive-resolver",
    "drive:restart": "cd docker && docker-compose restart drive-resolver",
    "drive:logs": "cd docker && docker-compose logs -f drive-resolver",
    "drive:health": "curl -s http://localhost:5000/health",
    "drive:stats": "curl -s http://localhost:5000/api/cache/stats",
    "drive:list": "curl -s http://localhost:5000/api/list",
    "drive:clear-cache": "curl -X POST http://localhost:5000/api/cache/clear",
    "drive:monitor": "powershell -ExecutionPolicy Bypass -File docker/drive-resolver/start.ps1 -Monitor"
  },
