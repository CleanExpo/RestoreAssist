  "testInstructions": {
    "description": "Use this test data to verify the NIR system functionality",
    "steps": [
      "1. Create a new report using the initialEntry data",
      "2. Fill in the NIR fields with the nirData provided",
      "3. Submit the inspection for processing",
      "4. Verify the classification matches expectedClassification",
      "5. Check that compliance requirements are triggered (asbestos, lead, electrical, mould) where applicable",
      "6. Verify scope items are automatically determined",
      "7. Check cost estimates are generated",
      "8. Generate the final report and verify it includes all standards references"
    ],
    "expectedBehaviors": {
      "classification": "System should automatically classify based on water source, contamination level, and affected area",
      "compliance": "System should trigger asbestos assessment for pre-1990 buildings, lead assessment for pre-1970 buildings, electrical assessment for water ingress, and mould testing for >3 days",
      "scopeDetermination": "System should automatically determine required scope items based on classification and building codes",
      "costEstimation": "System should calculate costs based on scope items and cost database",
      "reportGeneration": "Report should reference IICRC S500, WHS Regulations 2011, NCC, and AS/NZS 3000 where applicable"
    }
  },
  "apiTestEndpoints": {
    "createReport": {
      "method": "POST",
      "endpoint": "/api/reports/initial-entry",
      "description": "Create initial report with test data"
    },
    "createInspection": {
      "method": "POST",
      "endpoint": "/api/inspections",
      "description": "Create inspection record linked to report"
    },
    "addEnvironmentalData": {
      "method": "POST",
      "endpoint": "/api/inspections/[id]/environmental",
      "description": "Add environmental readings"
    },
    "addMoistureReadings": {
      "method": "POST",
      "endpoint": "/api/inspections/[id]/moisture",
      "description": "Add moisture readings"
    },
    "addAffectedAreas": {
      "method": "POST",
      "endpoint": "/api/inspections/[id]/affected-areas",
      "description": "Add affected area information"
    },
    "addScopeItems": {
      "method": "POST",
      "endpoint": "/api/inspections/[id]/scope-items",
      "description": "Add initial scope items"
    },
    "submitInspection": {
      "method": "POST",
      "endpoint": "/api/inspections/[id]/submit",
      "description": "Submit inspection for processing (triggers classification, scope determination, cost estimation)"
    },
    "getClassification": {
      "method": "GET",
      "endpoint": "/api/inspections/[id]/classification",
      "description": "Retrieve classification results"
    },
    "generateReport": {
      "method": "POST",
      "endpoint": "/api/reports/generate-inspection-report",
      "description": "Generate final report (Basic or Enhanced)"
    }
  }
